proj_name       = mkcdpg

mkcdpg_autodepends = .AUTODEPEND

inc_dirs        = -I"$(wrc_dir)/h"

!include cproj.mif
!include defrule.mif
!include deftarg.mif

.c: ..

objs = mkcdpg.obj

$(proj_name).exe : $(objs) $(__MAKEFILES__)
    @%make echo_link
!ifndef __WATCOM_TOOLS__
    $(cl) $(clflags) $(objs) $(ldflags)
!else
    $(linker) name $^@ $(lflags) file {$(objs)}
!endif
