proj_name = aliasgen

name = aliasgen

aliasgen_autodepends = .autodepend

!include cproj.mif
!include defrule.mif
!include deftarg.mif

.c: ../c

objs = $(name).obj

$(name).exe : $(objs) $(__MAKEFILES__)
    @%make echo_link
!ifndef __WATCOM_TOOLS__
    $(cl) $(clflags) $(objs) $(ldflags)
!else
    $(linker) name $@ $(lflags) file {$(objs)}
!endif
