#pmake: build os_os2 lcl 32bit support x86
#
# Builds wdsplice.dll
#

host_os  = os2
host_cpu = 386

proj_name = wdsplice

!ifndef wdsplice_autodepends
wdsplice_autodepends = .AUTODEPEND
!endif

sys_dll  = 1

!include cproj.mif
!include defrule.mif
!include deftarg.mif

objs = splice.obj

extra_cpp_flags = -DMD_x86

extra_l_flags_dll = op oneautodata

inc_dirs = -I"../h" -I"$(trap_dir)/h" -I"$(dig_dir)/h"

.c: ../c

wdsplice.dll : $(objs) $(__MAKEFILES__)
    @%make echo_link
    $(linker) name $@ $(lflags) file {$(objs)}
