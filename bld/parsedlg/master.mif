proj_name = parsedlg

parsedlg_autodepends = .autodepend

!include cproj.mif
!include defrule.mif
!include deftarg.mif

objs = parsedlg.obj

inc_dirs = -I"../h" -I"$(watcom_dir)/h"

.c: ../c

$(proj_name).exe : $(objs) $(__MAKEFILES__)
    @%make echo_link
!ifndef __WATCOM_TOOLS__
    $(cl) $(clflags) $(objs) $(ldflags)
!else
    $(linker) name $@ $(lflags) file {$(objs)}
!endif
